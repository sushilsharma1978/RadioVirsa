        <activity
            android:name=".Contact"
            android:screenOrientation="portrait" />
        <activity
            android:name=".YoutubeActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".YoutubePlayervideos"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ActivityStream"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ActivityExoplayer"
            android:screenOrientation="portrait" />
        <activity
            android:name=".VideoPlaylist"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Main2Activity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Mediaplayer"
            android:screenOrientation="portrait" />
        <activity
            android:name=".RadioClass"
